<%- include("layout", {
title: "Tickets",
body: `
<h1 class="text-3xl font-bold mb-6">Tickets</h1>

<table class="w-full bg-gray-800 rounded-xl overflow-hidden">
<thead class="bg-gray-700">
<tr>
    <th class="p-3">ID</th>
    <th class="p-3">User</th>
    <th class="p-3">Type</th>
    <th class="p-3">Status</th>
    <th class="p-3">Open</th>
</tr>
</thead>

<tbody>
<% tickets.forEach(t => { %>
<tr class="border-b border-gray-700">
    <td class="p-3"><%= t.id %></td>
    <td class="p-3"><%= t.username %></td>
    <td class="p-3"><%= t.type %></td>
    <td class="p-3"><%= t.status %></td>
    <td class="p-3">
        <a href="https://discord.com/channels/<%= guild %>/<%= t.channel %>"
           class="bg-indigo-600 px-3 py-1 rounded">Open</a>
    </td>
</tr>
<% }) %>
</tbody>
</table>

`
}) %>
